@using Reminders.Controllers
@model IEnumerable<Reminder>
@{
    ViewBag.Title = "Home Page";

}

<div class="jumbotron">
    <h1>ASP.NET</h1>
</div>

<div class="row">
    <div class="col-lg-12">
        <ul class="list-group">
            @foreach (var item in Model)
            {
                <li class="list-group-item">
                    <a href="~/Reminders">@item.Name</a>
                </li>
            }
        </ul>
    </div>
</div>